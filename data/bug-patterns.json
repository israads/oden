  {
    "id": "port-already-in-use",
    "name": "Port Already In Use",
    "category": "network",
    "description": "Development server port is already occupied by another process",
    "keywords": ["port", "already", "use", "EADDRINUSE", "listen", "address", "busy"],
    "errorSignatures": [
      {
        "pattern": "Error: listen EADDRINUSE.*:(\\d+)",
        "weight": 0.8
      },
      {
        "pattern": "Port \\d+ is already in use",
        "weight": 0.9
      },
      {
        "pattern": "EADDRINUSE: address already in use",
        "weight": 0.8
      },
      {
        "pattern": "something is already running on port \\d+",
        "weight": 0.7
      }
    ],
    "frameworks": ["react", "next.js", "express", "fastify", "vue", "angular"],
    "solution": {
      "description": "Kill process using the port and restart development server",
      "steps": [
        {
          "type": "command",
          "description": "Find process using port 3000",
          "command": "lsof -ti:${port} || netstat -tlnp | grep :${port}",
          "timeout": 5000
        },
        {
          "type": "command",
          "description": "Kill process using the port",
          "command": "pkill -f \"node.*${port}\" || kill -9 $(lsof -ti:${port}) 2>/dev/null || true",
          "timeout": 5000
        },
        {
          "type": "env_update",
          "description": "Set alternative port in environment",
          "file": ".env.local",
          "variables": {
            "PORT": "${alternativePort}"
          }
        }
      ],
      "affectedFiles": [".env.local"]
    },
    "validation": {
      "command": "npm start",
      "timeout": 15000,
      "successMessage": "Server started successfully",
      "successPattern": "ready|listening|compiled|started"
    },
    "successRate": 0.92,
    "totalApplications": 145,
    "successfulApplications": 133,
    "created": "2026-02-18T00:00:00.000Z",
    "lastUpdated": "2026-02-18T00:00:00.000Z"
  },

